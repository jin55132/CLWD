<Window x:Class="CLWD.View.GoogleOAuth2Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:ns="clr-namespace:CLWD.DP"
      Height="640" Width="640"
	Title="GoogleOAuth2Login" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow">

    <Grid>
        <WebBrowser Name="webBrowser1" ns:WebBrowserUtility.BindableSource="{Binding AuthorizationURI}" LoadCompleted="webBrowser1_LoadCompleted"/>
    </Grid>
</Window>
